<Window x:Class="WpfWinformIntegrationTests.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWinformIntegrationTests"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Click="OpenDialog">Click me to show a dialog</Button>
            <TextBlock Name="txtResult" />
            <Button Click="OpenForm">Click me to show a new form</Button>
        </StackPanel>
        
    </Grid>
</Window>
